{
  "name": "juke-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:prod",
    "build:bundle": "tsc -b && vite build",
    "build:dev": "cross-env JUKE_RUNTIME_ENV=development npm run build:bundle",
    "build:staging": "cross-env JUKE_RUNTIME_ENV=staging npm run build:bundle",
    "build:prod": "cross-env JUKE_RUNTIME_ENV=production npm run build:bundle",
    "preview": "vite preview",
    "test": "vitest --run",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --max-warnings=0 --ext .ts,.tsx src"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-essentials": "^8.1.0",
    "@storybook/addon-links": "^8.1.0",
    "@storybook/react": "^8.1.0",
    "@storybook/react-vite": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.2",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^24.0.0",
    "storybook": "^8.1.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  }
}
